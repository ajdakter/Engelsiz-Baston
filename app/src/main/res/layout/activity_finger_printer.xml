<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/finger_printer_design"
    tools:context=".login.finger_printer.FingerPrinterActivity">

    <ImageView
        android:id="@+id/fingerprintImage"
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:layout_marginStart="85dp"
        android:layout_marginTop="264dp"
        android:layout_marginEnd="85dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.494"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/group_2" />

    <TextView
        android:id="@+id/paraLabel"
        android:layout_width="match_parent"
        android:layout_height="150dp"
        android:layout_marginBottom="99dp"
        android:gravity="center"
        android:padding="20dp"
        android:textColor="#FFFFFF"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/fingerprintImage" />
</androidx.constraintlayout.widget.ConstraintLayout>